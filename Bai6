s1=input("Nhập chuỗi 1:")
s2=input("Nhập chuỗi 2:")
n,m=len(s1),len(s2)
dp = [[0]*(m+1) for _ in range(n+1)]
dai,cuoi=0,0
for i in range(1,n+1):
    for j in range(1,m+1):
        if s1[i-1]==s2[j-1]:
            dp[i][j]=dp[i-1][j-1]+1
            if dp[i][j]>dai:
                dai=dp[i][j]
                cuoi=i
print("Kết quả:",s1[cuoi-dai:cuoi])